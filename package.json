{
  "name": "michaelajaritz",
  "version": "1.0.0",
  "description": "The personal website of Michaela Jaritz, business coach and trainer.",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ./**/*.js",
    "lint-fix": "eslint --fix ./**/*.js",
    "test": "jest",
    "generate-coverage-report": "jest --coverage",
    "deploy-coverage-report": "(cd ./coverage/lcov-report/ && now && now alias)",
    "copy-coverage-now-config": "cp ./now.coverage.json ./coverage/lcov-report/now.json",
    "publish-coverage-report": "npm run generate-coverage-report && npm run copy-coverage-now-config && npm run deploy-coverage-report",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish-website": "now && now alias"
  },
  "repository": {
    "type": "git",
    "url": "git+https://vogelino@bitbucket.org/vogelino/michaelajaritz.git"
  },
  "author": "Lucas Vogel <contact@vogelino.com> (http://vogelino.com/)",
  "license": "ISC",
  "homepage": "https://bitbucket.org/vogelino/michaelajaritz#readme",
  "dependencies": {
    "next": "^3.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-intl": "^2.3.0",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/react": "^3.2.8",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-vogelint-react": "^1.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "now": "^7.4.0",
    "react-test-renderer": "^15.6.1",
    "regenerator-runtime": "^0.11.0"
  }
